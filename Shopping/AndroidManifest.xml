<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kmanikumarreddy.shopping"
    android:versionCode="1"
    android:versionName="1.0" android:installLocation="auto">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kmanikumarreddy.shopping.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kmanikumarreddy.shopping.RateSite"
            android:label="@string/title_activity_rate_site" >
            <intent-filter>
                <action android:name="com.kmanikumarreddy.shopping.RateSite"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.kmanikumarreddy.shopping.BuyProducts"
            android:label="@string/title_activity_buy_products" >
            <intent-filter>
                <action android:name="com.kmanikumarreddy.shopping.BuyProducts"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
